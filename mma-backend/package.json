  "version": "1.0.0",
  "description": "UFC Fighter Management Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "prisma migrate dev",
    "seed": "prisma db seed",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "auto-enhance": "ts-node scripts/autoEnhance.ts",
