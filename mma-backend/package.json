  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "node-html-parser": "^7.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.26.0",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
